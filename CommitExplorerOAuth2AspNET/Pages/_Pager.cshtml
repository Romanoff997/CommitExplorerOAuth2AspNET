@model CommitExplorerOAuth2AspNET.Models.Pager.ListPager

<div class="container">

    @if (Model.TotalPages > 0)
    {
        <ul class="pagination justify-content-end">
            @if (Model.CurrentPage > 1)
            {
                <li class="page-item">
                    <button class="page-link" click="Pager(1)">First</button>
                </li>
                <li class="page-item">
                    <button class="page-link" onclick="Pager(@Model.CurrentPage-1)">Previous</button>
                </li>

            }
            @for (var pge = Model.StartPage; pge <= Model.EndPage; pge++)
            {
                <li class="page-item @(pge ==Model.CurrentPage ? "active": "")">
                    <button class="page-link" onclick="Pager(@pge)">@pge</button>
                </li>

            }
            @if (Model.CurrentPage < Model.TotalPages)
            {
                <li class="page-item">
                    <button class="page-link" onclick="Pager(@Model.CurrentPage+1)">Next</button>
                </li>
                <li class="page-item">
                    <button class="page-link" onclick="Pager(@Model.TotalPages)">Last</button>
                </li>

            }
        </ul>
    }
</div>

